{
    "title": "Disqus",
    "name": "gitbook-plugin-disqus",
    "description": "Disqus thread in the footer of your GitBook",
    "icon": "./icon.png",
    "main": "index.js",
    "version": "0.1.0",
    "engines": {
        "gitbook": ">=4.0.0-alpha"
    },
    "homepage": "https://github.com/GitbookIO/plugin-disqus",
    "repository": {
        "type": "git",
        "url": "https://github.com/GitbookIO/plugin-disqus.git"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/GitbookIO/plugin-disqus/issues"
    },
    "keywords": [
        "gitbook:social"
    ],
    "gitbook": {
        "properties": {
            "shortName": {
                "type": "string",
                "title": "Website Shortname",
                "required": true
            },
            "useIdentifier": {
                "type": "boolean",
                "title": "Use page identifier",
                "default": false
            }
        }
    }
}
